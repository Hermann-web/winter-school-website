                source(src="assets/visual/teaser-v5.webm" type="video/webm")
                source(src="assets/visual/teaser-v5.ogv" type="video/ogg")

            // Dark Overlay
            .background-overlay(style="position: absolute; top: 0; left: 0; right: 0; bottom: 0; z-index: -1; background-color: rgba(0, 0, 0, 0.6);")
        
