                        .timeline-item.feature-hover.mt-4.d-flex.align-items-baseline.flex-column.flex-md-row.row
                            .time-header.col-12.col-md-2.bg-dark.text-white.p-2.rounded-pill 15:00 - 16:00
                            .theme-header.col-12.col-md-10.fs-5.align-self-center.text-dark.font-weight-bold Socio-Economic Impact of Next-Generation AI
                            .col-0.col-md-2
                            .col-12.col-md-10.text-muted
