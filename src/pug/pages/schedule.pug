extends ../layouts/dashboard.pug

block config
    - var bodyClass = 'sb-nav-fixed'
    - var pageTitle = 'Programme'
    - var pageMainTitle = 'Schedule'
    - var pageSubTitle = "A preliminary schedule, subject to final approval from both participants and organizers"
    - var sidenavStyle = 'sb-sidenav-dark'

block content
    .container-fluid.min-vh-100
        .row.mt-5
            .col-0.col-lg-1 &nbsp;
            .col-12.col-lg-10.mt-3.card.mb-4.border-0
                include includes/page-header.pug
                .card-body
                    .container.timeline

                        // Day 1 - 24 February 2025
                        h3.text-center.text-primary.mt-5 Day 1: February 24, 2025 (EMINES Conference Center)
                        h4.text-center.text-muted Next Generation AI and Economic Applications
                        hr

                        .timeline-item.mt-4.d-flex.align-items-baseline.flex-column.flex-md-row.row
                            .time-header.col-12.col-md-2.bg-primary.text-white.p-2.rounded-pill 09:00 - 09:30
                            .theme-header.col-12.col-md-10.fs-5.align-self-center.text-primary Welcome and Opening Speech

                        .timeline-item.mt-4.d-flex.align-items-baseline.flex-column.flex-md-row.row
                            .time-header.col-12.col-md-2.bg-secondary.text-dark.p-2.rounded-pill 09:30 - 11:30
                            .theme-header.col-12.col-md-10.fs-5.align-self-center.text-secondary AI and Financial Applications
                            .col-0.col-md-2
                            .col-12.col-md-10.text-gray
                                span Charles-Albert Lehalle, Professor, École Polytechnique (l’X)

                        .timeline-item.mt-4.d-flex.align-items-baseline.flex-column.flex-md-row.row
                            .time-header.col-12.col-md-2.bg-secondary.text-dark.p-2.rounded-pill 09:45 - 10:45
                            .theme-header.col-12.col-md-10.fs-5.align-self-center.text-secondary Generative AI: A Deep Dive into Diffusion Models
                            .col-0.col-md-2
                            .col-12.col-md-10.text-gray
                                span Eric Moulines, Professor, École Polytechnique (l’X)

                        .timeline-item.mt-4.d-flex.align-items-baseline.flex-column.flex-md-row.row
                            .time-header.col-12.col-md-2.bg-secondary.text-dark.p-2.rounded-pill 11:00 - 12:00
                            .theme-header.col-12.col-md-10.fs-5.align-self-center.text-secondary An Alternative View on AI: Collaborative Learning, Incentives, and Social Welfare
                            .col-0.col-md-2
                            .col-12.col-md-10.text-gray
                                span Michael Jordan, Professor, Inria and MBZUAI

                        .timeline-item.mt-4.d-flex.align-items-baseline.flex-column.flex-md-row.row
                            .time-header.col-12.col-md-2.bg-secondary.text-dark.p-2.rounded-pill 11:45 - 12:30
                            .theme-header.col-12.col-md-10.fs-5.align-self-center.text-secondary AI in Morocco
                            .col-0.col-md-2
                            .col-12.col-md-10.text-gray
                                span AI Movement

                        .timeline-item.mt-4.d-flex.align-items-baseline.flex-column.flex-md-row.row
                            .time-header.col-12.col-md-2.bg-primary.text-white.p-2.rounded-pill 12:30 - 14:00
                            .theme-header.col-12.col-md-10.fs-5.align-self-center.text-primary Lunch

                        .timeline-item.mt-4.d-flex.align-items-baseline.flex-column.flex-md-row.row
                            .time-header.col-12.col-md-2.bg-secondary.text-dark.p-2.rounded-pill 14:00 - 14:50
                            .theme-header.col-12.col-md-10.fs-5.align-self-center.text-secondary Diffusion Models at Work: Deep Prior for Inverse Problems
                            .col-0.col-md-2
                            .col-12.col-md-10.text-gray
                                span Eric Moulines

                        // Day 2 - 25 February 2025
                        h3.text-center.text-primary.mt-5 Day 2: February 25, 2025 (EMINES Conference Center)
                        //- h4.text-center.text-muted EMINES Conference Center
                        h4.text-center.text-muted Next Generation AI and Economic Applications
                        hr

                        .timeline-item.mt-4.d-flex.align-items-baseline.flex-column.flex-md-row.row
                            .time-header.col-12.col-md-2.bg-secondary.text-dark.p-2.rounded-pill 14:00 - 16:00
                            .theme-header.col-12.col-md-10.fs-5.align-self-center.text-secondary Socio-Economic Impact of Next-Generation AI
                            .col-0.col-md-2
                            .col-12.col-md-10.text-gray
                                span Topics:
                                ul
                                    li Transformation of Key Economic Sectors Through AI
                                    li Innovation and Economic Inequalities
                                    li Impact on Entrepreneurship and Technological Innovation
                                span Speaker: Benoit Bergeret

                        .timeline-item.mt-4.d-flex.align-items-baseline.flex-column.flex-md-row.row
                            .time-header.col-12.col-md-2.bg-secondary.text-dark.p-2.rounded-pill 15:00 - 16:00
                            .theme-header.col-12.col-md-10.fs-5.align-self-center.text-secondary Recent Advancements in AI and Major Challenges
                            .col-0.col-md-2
                            .col-12.col-md-10.text-gray
                                span Yann LeCun

                        .timeline-item.mt-4.d-flex.align-items-baseline.flex-column.flex-md-row.row
                            .time-header.col-12.col-md-2.bg-secondary.text-dark.p-2.rounded-pill 16:10 - 17:10
                            .theme-header.col-12.col-md-10.fs-5.align-self-center.text-secondary Round Table 1: Bridging Theory and Applications in Next-Generation AI
                            .col-0.col-md-2
                            .col-12.col-md-10.text-gray
                                span Panelists:
                                ul
                                    li Yann LeCun
                                    li Michael Jordan
                                    li Eric Moulines

                        .timeline-item.mt-4.d-flex.align-items-baseline.flex-column.flex-md-row.row
                            .time-header.col-12.col-md-2.bg-secondary.text-dark.p-2.rounded-pill 16:10 - 18:10
                            .theme-header.col-12.col-md-10.fs-5.align-self-center.text-secondary AI in Motion: The Future of Mobility + Hackathon
                            .col-0.col-md-2
                            .col-12.col-md-10.text-gray
                                span Luciano Borges, CEO SIANA, + Young Innovators

                        .timeline-item.mt-4.d-flex.align-items-baseline.flex-column.flex-md-row.row
                            .time-header.col-12.col-md-2.bg-primary.text-white.p-2.rounded-pill 17:15 - 18:00
                            .theme-header.col-12.col-md-10.fs-5.align-self-center.text-primary Poster Session

                        div
                            a.btn.btn-outline-primary.mt-2.w-100(href="./assets/visual/schedule-ai-conference-2025.pdf" target="_blank" download="Next-Generation-AI-2025-Programme")
                                i.fas.fa-file-pdf
                                |  Download the full schedule
            .col-0.col-md-1 &nbsp;
