                        .timeline-item.feature-hover.mt-4.d-flex.align-items-baseline.flex-column.flex-md-row.row
                            .time-header.col-12.col-md-2.bg-dark.text-white.p-2.rounded-pill 11:00 - 11:45
                            .theme-header.col-12.col-md-10.fs-5.align-self-center.text-dark.font-weight-bold Data and Digital Transformation @ Carrefour
                            .col-0.col-md-2
                            .col-12.col-md-10.text-muted
                                span Emmanuel Grenier, Executive Director of E-Commerce, Data and Digital Transformation, Carrefour

                        .timeline-item.feature-hover.mt-4.d-flex.align-items-baseline.flex-column.flex-md-row.row
                            .time-header.col-12.col-md-2.bg-dark.text-white.p-2.rounded-pill 11:45 - 12:30
                            .theme-header.col-12.col-md-10.fs-5.align-self-center.text-dark.font-weight-bold AI in Morocco
                            .col-0.col-md-2
                            .col-12.col-md-10.text-muted
                                span AI Movement

                        .timeline-item.feature-hover.mt-4.d-flex.align-items-baseline.flex-column.flex-md-row.row
                            .time-header.col-12.col-md-2.bg-secondary.text-white.p-2.rounded-pill 12:30 - 14:00
                            .theme-header.col-12.col-md-10.fs-5.align-self-center.text-secondary.font-weight-bold Lunch

                        .timeline-item.feature-hover.mt-4.d-flex.align-items-baseline.flex-column.flex-md-row.row
                            .time-header.col-12.col-md-2.bg-light.text-white.p-2.rounded-pill 14:00 - 14:45
                            .theme-header.col-12.col-md-10.fs-5.align-self-center.text-light.font-weight-bold AI in Motion: The Future of Mobility + Best Projects Hackathon “Rail Summit” Exhibition
