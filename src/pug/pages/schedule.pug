                        .timeline-item.feature-hover.mt-4.d-flex.align-items-baseline.flex-column.flex-md-row.row
                            .time-header.col-12.col-md-2.bg-light.text-white.p-2.rounded-pill 17:15 - 18:00
                            .theme-header.col-12.col-md-10.fs-5.align-self-center.text-light.font-weight-bold Poster Session

