extends ../layouts/dashboard.pug

block config
    - var bodyClass = 'sb-nav-fixed'
    - var pageTitle = 'Programme'
    - var pageMainTitle = 'Schedule'
    - var pageSubTitle = "A preliminary schedule, subject to final approval from both speackers and organizers"
    - var sidenavStyle = 'sb-sidenav-dark'

block content
    .container-fluid.min-vh-100
        .row.mt-5
            .col-0.col-lg-1 &nbsp;
            .col-12.col-lg-10.mt-3.card.mb-4.border-0
                include includes/page-header.pug
                .card-body
                    .container.timeline.hover-effect

                        // Day 1 - 24 February 2025
                        h3.text-center.text-light.mt-5 Day 1: February 24, 2025 (EMINES Conference Center)
                        h4.text-center.text-muted.mb-4 Next Generation AI and Economic Applications
                        hr

                        .timeline-item.feature-hover.mt-4.d-flex.align-items-baseline.flex-column.flex-md-row.row
                            .time-header.col-12.col-md-2.bg-secondary.text-white.p-2.rounded-pill 09:00 - 09:30
                            .theme-header.col-12.col-md-10.fs-5.align-self-center.text-secondary.font-weight-bold Welcome and Opening Speech

                        .timeline-item.feature-hover.mt-4.d-flex.align-items-baseline.flex-column.flex-md-row.row
                            .time-header.col-12.col-md-2.bg-dark.text-white.p-2.rounded-pill 09:45 - 10:45
                            .theme-header.col-12.col-md-10.fs-5.align-self-center.text-dark.font-weight-bold Generative AI: A Deep Dive into Diffusion Models
                            .col-0.col-md-2
                            .col-12.col-md-10.text-muted
                                span Eric Moulines, Professor, École Polytechnique (l’X)

                        .timeline-item.feature-hover.mt-4.d-flex.align-items-baseline.flex-column.flex-md-row.row
                            .time-header.col-12.col-md-2.bg-dark.text-white.p-2.rounded-pill 11:00 - 12:00
                            .theme-header.col-12.col-md-10.fs-5.align-self-center.text-dark.font-weight-bold An Alternative View on AI: Collaborative Learning, Incentives, and Social Welfare
                            .col-0.col-md-2
                            .col-12.col-md-10.text-muted
                                span Michael Jordan, Prof at University of California, Berkeley and MBZUAI, Researcher at Inria

                        .timeline-item.feature-hover.mt-4.d-flex.align-items-baseline.flex-column.flex-md-row.row
                            .time-header.col-12.col-md-2.bg-secondary.text-white.p-2.rounded-pill 12:30 - 14:00
                            .theme-header.col-12.col-md-10.fs-5.align-self-center.text-secondary.font-weight-bold Lunch

                        .timeline-item.feature-hover.mt-4.d-flex.align-items-baseline.flex-column.flex-md-row.row
                            .time-header.col-12.col-md-2.bg-dark.text-white.p-2.rounded-pill 14:00 - 14:55
