                    //-             .card.feature-hover
                    //-                 .card-body
                    //-                     img(src=`assets/firms/${partner.img}`, alt=partner.name, class="img-fluid mb-3")
                    //-                     if partner.type
                    //-                         p.text-muted= partner.type
                    //-                     if partner.url
