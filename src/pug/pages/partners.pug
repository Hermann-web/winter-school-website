extends ../layouts/dashboard.pug

block config
    - var bodyClass = 'sb-nav-fixed'
    - var pageTitle = 'Partners'
    - var pageMainTitle = 'Our Partners'
    - var pageSubTitle = 'Sponsors and Collaborators who make this event possible'
    - var sidenavStyle = 'sb-sidenav-dark'
    - var SponsorsList = [{ name: 'EMINES', img: 'emines.png', type: 'Title Sponsor', description: 'Industrial management-focused engineering school in Morocco.' }, { name: 'OCP', img: 'ocp.png', type: 'Gold Sponsor', description: 'Global leader in phosphates and AI applications in agriculture.' }, { name: 'École Polytechnique (l’X)', img: 'polytechnique-rmbg.png', type: 'Academic Partner', description: 'Prestigious French institution for science and engineering.' }, { name: 'SIANA', img: 'siana.png', type: 'Bronze Sponsor', description: 'Innovators in high-speed rail and transportation AI solutions.' },]
